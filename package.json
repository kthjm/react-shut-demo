{
  "scripts": {
    "format": "prettier --write **/*.js",
    "dev:chin": "chin development -v -c script/chin.config.json -p script/chin.preset.js",
    "dev:dll": "webpack --config script/webpack.config.dll.babel.js",
    "dev:pre": "run-p dev:chin dev:dll",
    "dev": "webpack-dev-server --config script/webpack.config.dev.babel.js",
    "pro:chin": "chin production -v -c script/chin.config.json -p script/chin.preset.js",
    "pro:pack": "webpack -p --config script/webpack.config.pro.babel.js",
    "pro": "run-p pro:chin pro:pack",
    "pro:serve": "http-server .site -a localhost -p 7000 -o -c-1"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxBracketSameLine": true
  },
  "dependencies": {
    "atra": "^1.1.2",
    "babel-polyfill": "^6.26.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-shut": "^2.0.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "chin": "^2.1.2",
    "favicons": "^4.8.6",
    "flow-bin": "^0.61.0",
    "flow-typed": "^2.2.1",
    "fs-extra": "^5.0.0",
    "http-server": "^0.10.0",
    "imagemin": "^5.3.1",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-pngquant": "^5.0.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.9.1",
    "pug": "^2.0.0-rc.2",
    "rehype": "^5.0.1",
    "rehype-format": "^2.2.0",
    "rehype-react": "^3.0.2",
    "remark": "^8.0.0",
    "remark-rehype": "^3.0.0",
    "rollup": "^0.52.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "uglify-es": "^3.2.2",
    "unified": "^6.1.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.0"
  }
}
